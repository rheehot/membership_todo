{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": 0,
    "object-curly-newline": 0,
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "no-return-await": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "no-undef": 0
  }
}
